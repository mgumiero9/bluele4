				System.out.println(De);
		         System.exit(1);  
			}
			
			System.out.println("Lista de Dispositivos");
			for(Iterator<String> i = bInterfaces.iterator(); i.hasNext(); ) {
				System.out.println(i);
			}
			System.out.println("Get Adapter OK ");
